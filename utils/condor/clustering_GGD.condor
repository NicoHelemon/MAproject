Executable = $(InputDir)/utils/condor/clustering_GGD.sh
InitialDir = $(InputDir)

Error = $(OutputDir)/logs/clustering_GGD/err.$(Process)
Log = $(OutputDir)/logs/clustering_GGD/log.$(Process)
