transfer_input_files = $(InputDir)/net.dat, $(InputDir)/results/clustering/GGD/full/graphs.csv
transfer_output_files = results
Arguments = -graphs_path results/clustering/GGD/full -toy True -print True -save True
Queue 1

# Condor process : 2
